                                        <Path.RenderTransform>
                                            <TransformGroup>
                                                <ScaleTransform x:Name="path" ScaleX="0"/>
                                                <SkewTransform/>
                                                <RotateTransform x:Name="arrow" Angle="0"/>
                                                <TranslateTransform/>
                                            </TransformGroup>
