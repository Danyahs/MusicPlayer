                        <Border BorderThickness="0,0,0,0.1" BorderBrush="#FFb3b3b3" Background="Transparent">
                            <Grid x:Name="border">
                                <Line x:Name="Line1" Grid.Column="0" Grid.ColumnSpan="6" Stroke="#FFb3b3b3" StrokeThickness="0.1" X1="0" Y1="19" X2="{Binding ElementName=border, Path=ActualWidth}" Y2="19" Margin="0,0,0,0"/>
