                                                <BeginStoryboard>
                                                    <Storyboard>
                                                        <StringAnimationUsingKeyFrames Storyboard.TargetName="PART_ContentHost" Storyboard.TargetProperty="Text" Duration="0:0:0">
                                                            <DiscreteStringKeyFrame KeyTime="0:0:0" Value=""/>
                                                        </StringAnimationUsingKeyFrames>
                                                    </Storyboard>
                                                </BeginStoryboard>
                                            </EventTrigger>

