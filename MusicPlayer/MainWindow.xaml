                                        <Path.Data>
                                            <PathGeometry>
                                                <PathGeometry.Figures>
                                                    <PathFigure StartPoint="7,12" IsClosed="True">
                                                        <LineSegment Point="14, 2"/>
                                                        <LineSegment Point="21, 12"/>
                                                        <LineSegment Point="14, 4"/>
                                                        <LineSegment Point="7, 12"/>
                                                    </PathFigure>
                                                </PathGeometry.Figures>
