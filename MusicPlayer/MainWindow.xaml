                            <ComboBox Grid.Row="1" VerticalAlignment="Bottom" Width="80" Margin="0,0,740,10"/>
                            <ListView local:GridViewSort.AutoSort="True" x:Name="TrackList" BorderThickness="0" Grid.Row="2" FontFamily="Circular std book" FontSize="14" Foreground="White" ScrollViewer.CanContentScroll="False" ScrollViewer.VerticalScrollBarVisibility="Disabled" ScrollViewer.HorizontalScrollBarVisibility="Disabled" Background="Transparent" Margin="30,0,30,0">
                                <ListView.Template>
